# Copy this file to .env.local and fill in your values

# Supabase (Required)
# Get from: https://supabase.com → Project Settings → API
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Exa API (Required)
# Get from: https://exa.ai → Dashboard → API Keys
EXA_API_KEY=

# Perplexity API (Required)
# Get from: https://perplexity.ai/settings/api
PERPLEXITY_API_KEY=

# Firecrawl API (Optional - scrapes websites for leadership data)
# Get from: https://firecrawl.dev
FIRECRAWL_API_KEY=
